{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-react"
    ],
    "jsRules": {},
    "rules": {
      "object-literal-sort-keys": false,
      "curly": [true, "ignore-same-line"],
      "interface-name": [true, "never-prefix"],
      "no-console": false,
      "ordered-imports": false,
      "arrow-parens": false
    },
    "rulesDirectory": []
}
